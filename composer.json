{
    "name": "pcode/google-font-downloader",
    "description": "Composer package google-font-downloader",
    "keywords": [
        "symfony",
        "composer",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "PCode",
            "email": "office@pcode.at"
        }
    ],
    "support": {
        "email": "office@pcode.at",
        "irc": "irc://irc.pcode.at/composer"
    },
    "require": {
        "php": ">=8",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "PCode\\GoogleFontDownloader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PCode\\GoogleFontDownloader\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always --testdox"
    }
}
